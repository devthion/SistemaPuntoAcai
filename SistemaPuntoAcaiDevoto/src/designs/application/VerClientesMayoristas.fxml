            <TableColumn fx:id="colBarrio" prefWidth="102.0" text="Barrio" />
            <TableColumn fx:id="colCalle" prefWidth="196.0" text="Calle" />
            <TableColumn fx:id="colNumero" text="Numero" />
            <TableColumn fx:id="colIngresos" prefWidth="161.0" text="Ingresos Generados" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
