            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="btnEditarCliente" layoutX="480.0" layoutY="533.0" mnemonicParsing="false" onAction="#editarCliente" prefHeight="51.0" prefWidth="340.0" stylesheets="@../Styles/Style.css" text="EDITAR CLIENTE" textFill="WHITE">
